<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <servlet>
    <servlet-name>FollowAdder</servlet-name>
    <servlet-class>eu.bigproject.dissemination.FollowAdder</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>FollowAdder</servlet-name>
    <url-pattern>/followadder</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CreateReport</servlet-name>
    <servlet-class>eu.bigproject.dissemination.CreateReport</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CreateReport</servlet-name>
    <url-pattern>/createreport</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list><welcome-file>index.html</welcome-file></welcome-file-list>
</web-app>    
